<article class="post">
     <div class="col s12 m5">
      <div class="card-panel light-blue lighten-2">
            <h3>Title:</h3>
            <h5>BC Name:</h5>
            <h5>Format:</h5>
            <h5>Length:</h5>
            <h5>Price:</h5>
            <h5>Repeat:</h5>
            <h5>Rating: </h5>
            <h5>Comments:</h5>
        </div>
        <div class="content">
            <div id="right" class="column right">
                <h2>{{title}}</h2>
                <h5>{{bootcampName}}</h5>
                <h5>{{deliverFormat}}</h5>
                <h5>{{length}}</h5>
                <h5>{{price}}</h5>
                <h5>{{repeat}}</h5>
                <h5>{{overallRating}}</h5>
                <h5>{{review_comments}}</h5>

            </div>
        </div>
    </div>
</article>
